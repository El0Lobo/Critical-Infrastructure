{
  "name": "terminal-logger",
  "displayName": "Terminal Logger",
  "description": "Log all VS Code terminal output to a file in the workspace.",
  "version": "0.0.1",
  "publisher": "local",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "terminalLogger.openLog",
        "title": "Terminal Logger: Open Log"
      }
    ]
  }
}
